{
  "name": "@creditkarma/consul-client",
  "version": "0.0.0",
  "description": "A client for Hashicorp Consul written in TypeScript",
  "main": "dist/main/index.js",
  "files": [
    "dist/main"
  ],
  "keywords": [
    "TypeScript",
    "Hashicorp",
    "Consul"
  ],
  "scripts": {
    "clean": "rimraf dist/",
    "lint": "tslint --fix './src/**/*.ts'",
    "docker": "docker-compose up consul &",
    "docker:kill": "docker-compose kill consul",
    "build": "npm run clean && npm run lint && tsc",
    "pretest": "npm run build && npm run docker",
    "test": "wait-on http://localhost:8500 && npm run lint && NODE_ENV=test mocha --opts mocha.opts",
    "posttest": "npm run docker:kill && rimraf consuldata/*"
  },
  "author": "Credit Karma",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/chai": "^4.0.5",
    "@types/debug": "0.0.30",
    "@types/mocha": "^2.2.44",
    "chai": "^4.1.2",
    "mocha": "^4.0.1",
    "rimraf": "^2.6.2",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "wait-on": "^2.0.2"
  },
  "dependencies": {
    "@types/node": "^8.0.51",
    "@types/request": "^2.0.8",
    "@types/request-promise-native": "^1.0.10",
    "debug": "^3.1.0",
    "just-maybe": "^1.0.6",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5"
  }
}
